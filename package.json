{
  "name": "starrail.js",
  "version": "1.5.0",
  "description": "A Node.js Enka.Network API wrapper for Honkai: Star Rail.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "lib": "src",
    "doc": "docs"
  },
  "scripts": {
    "install": "node scripts/install.js",
    "lint": "npx eslint . --config .eslintrc.json --ext .js,.jsx,.cjs,.mjs,.ts,.tsx --ignore-path .eslintignore",
    "lint:fix": "npx eslint . --config .eslintrc.json --ext .js,.jsx,.cjs,.mjs,.ts,.tsx --ignore-path .eslintignore --fix",
    "build": "node scripts/clean.js && npx tsc",
    "build:debug": "node scripts/clean.js && npx tsc --sourceMap",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "starrail",
    "star rail",
    "honkai",
    "api",
    "mihoyo",
    "enka",
    "enka.network",
    "hoyoverse"
  ],
  "author": "yuko1101 (https://github.com/yuko1101)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuko1101/starrail.js.git"
  },
  "homepage": "https://starrail.vercel.app",
  "license": "MIT",
  "devDependencies": {
    "@types/unzip-stream": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^9.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "config_file.js": "^1.2.4",
    "enka-system": "^0.3.4",
    "unzip-stream": "^0.3.1"
  }
}
